{
	"name": "novely-story-highlight",
	"displayName": "Novely Story Language Highlight",
	"description": "Highlighting for format for novel stories",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.79.0"
	},
	"categories": [
		"Programming Languages"
	],
	"publisher": "yhdgms1",
	"repository": {
		"url": "https://github.com/yhdgms1/novely/tree/main/packages/vscode-highlight"
	},
	"contributes": {
		"languages": [
			{
				"id": "nvly",
				"aliases": [
					"Novely",
					"nvly",
					"nvl"
				],
				"extensions": [
					".nvl",
					".nvly",
					".novely"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "nvly",
				"scopeName": "novely",
				"path": "./syntaxes/nvly.tmLanguage.json"
			}
		]
	}
}
