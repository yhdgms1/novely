{
	"name": "@novely/live2d",
	"description": "Integration of live2d for novely",
	"version": "0.2.0",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
